@model IEnumerable<WHouse.Models.WarehouseAddition>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
</head>
<body>
<div class="container">
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.OrderConfirm.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Userr.name)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.OrderConfirm.name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Userr.name)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new {id = item.additionNumer}) |
                @Html.ActionLink("Details", "Details", new {id = item.additionNumer}) |
                @Html.ActionLink("Delete", "Delete", new {id = item.additionNumer})
            </td>
        </tr>
    }

</table>
</div>
</body>
</html>
